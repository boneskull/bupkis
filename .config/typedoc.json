{
  "$schema": "https://typedoc.org/schema.json",
  "cleanOutputDir": true,
  "customCss": "./custom.css",
  "entryPoints": ["../src/index.ts"],
  "excludeInternal": true,
  "excludePrivate": true,
  "externalSymbolLinkMappings": {
    "@types/node": {
      "assert.AssertionError": "https://nodejs.org/api/errors.html#class-assertionerror",
      "assert.strictEqual": "https://nodejs.org/api/assert.html#assertstrictequalactual-expected-message"
    },
    "zod": {
      "ZodError": "https://zod.dev/packages/core#errors",
      "ZodType": "https://zod.dev/packages/core#schemas",
      "ZodPromise": "https://zod.dev/packages/core#schemas"
    },
    "type-fest": {
      "Constructor": "https://github.com/sindresorhus/type-fest/blob/main/source/basic.d.ts"
    }
  },

  "hostedBaseUrl": "https://boneskull.github.io/bupkis",
  "kindSortOrder": [
    "Reference",
    "Project",
    "Module",
    "Namespace",
    "Function",
    "TypeAlias"
  ],
  "modifierTags": [
    "@knipignore",
    "@assertion",
    "@subject",
    "@value",
    "@phrase",
    "@slots"
  ],
  "navigationLinks": {
    "GitHub": "https://github.com/boneskull/bupkis"
  },
  "out": "../docs",
  "plugin": [
    "typedoc-github-theme",
    "typedoc-plugin-mdn-links",
    "typedoc-plugin-zod"
  ],
  "sort": ["kind"],
  "tsconfig": "./tsconfig.typedoc.json"
}
